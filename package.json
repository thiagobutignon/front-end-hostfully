{
  "name": "boiler-plate-react-clean-architecture",
  "version": "1.0.0",
  "private": true,
  "description": "a really cool app",
  "engines": {
    "node": "18.x",
    "yarn": "1.22.17",
    "npm": "10.2.4"
  },
  "scripts": {
    "start": "node index",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "dev:base": "webpack-dev-server --config webpack.dev.js",
    "dev": "node init.js select",
    "build": "webpack --config webpack.prod.js",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --passWithNoTests --runInBand",
    "test:watch": "npm test -- --watch --testMatch='**/*.spec.ts' --testMatch='**/*.spec.tsx'",
    "test:integration": "npm test -- --watch --testMatch='**/*.test.ts' --testMatch='**/*.test.tsx'",
    "test:watch:data": "jest --watch --testMatch=\"<rootDir>/tests/data/**/*.spec.ts\"",
    "test:staged": "npm test -- --no-cache --runInBand --findRelatedTests",
    "test:ci": "npm test -- --coverage --testMatch='**/*.spec.ts' --testMatch='**/*.spec.tsx'",
    "check": "npm-check -u -s",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "css-modules-typescript-loader": "^4.0.1",
    "date-fns": "^3.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-history-api-fallback": "^2.2.1",
    "framer-motion": "^10.6.8",
    "jest-transform-stub": "^2.0.0",
    "react": "^18",
    "react-date-range": "^2.0.0-alpha.4",
    "react-datepicker": "^6.1.0",
    "react-dom": "^18",
    "react-icons": "^4.12.0",
    "react-router-dom": "^5.3.4",
    "sass": "^1.68.0",
    "sharp": "^0.33.2",
    "swiper": "^11.0.5"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@faker-js/faker": "^8.3.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.18",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.42",
    "@types/react-date-range": "^1.4.9",
    "@types/react-datepicker": "^4.19.5",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.1.5",
    "@types/swiper": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/experimental-utils": "^5.62.0",
    "@typescript-eslint/parser": "^6.13.2",
    "clean-webpack-plugin": "^4.0.0",
    "commander": "^11.1.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "favicons": "^7.1.5",
    "favicons-webpack-plugin": "^6.0.1",
    "full-icu": "^1.5.0",
    "git-commit-msg-linter": "^5.0.4",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "jest-transform-css": "^6.0.1",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "prettier": "^3.0.3",
    "start-server-and-test": "^2.0.3",
    "style-loader": "^3.3.3",
    "swiper-react": "^1.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./commit-lint-config-convetional"
    }
  }
}
